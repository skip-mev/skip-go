{
  "name": "@skip-go/web-component",
  "description": "Web Component version of the skip go widget",
  "version": "2.4.3",
  "repository": "https://github.com/skip-mev/widget",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "types": "./build/index.d.ts",
  "engines": {
    "node": ">=12.4"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "start": "rollup --config rollup.config.js -w",
    "dev": "rollup --config rollup.config.js -w",
    "build": "NODE_OPTIONS=--max-old-space-size=16384 rollup --config rollup.config.js",
    "lint": "eslint src --ext .ts",
    "postpublish": "yarn run post",
    "prepublishOnly": "yarn run pre",
    "prepack": "yarn run pre",
    "postpack": "yarn run post",
    "pre": "npm run build && node scripts/prepublish.cjs",
    "post": "git checkout -- package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@skip-go/widget": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-internal": "^1.0.4",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^5.12.0"
  }
}
